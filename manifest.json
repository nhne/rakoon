{

  "manifest_version": 2,
  "name": "neogulman",
  "version": "1.1.5",

  "description": "it will help save your time.",

  "icons": {
    "48": "images/48.png",
    "96": "images/96.png"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "images/48.png",
    "default_title": "rakoon",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches" : ["*://helplms.snu.ac.kr/learning/*"],
      "js" : ["neogul.js"],
      "css" : ["neogul.css"]
    }
  ]
}
