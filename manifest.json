{

  "manifest_version": 2,
  "name": "neogulman",
  "version": "1.1",

  "description": "it will help save your time.",

  "icons": {
    "48": "48.jpg",
    "96": "96.jpg"
  },

  "permissions": [
    "activeTab"
  ],

  "browser_action": {
    "default_icon": "48.jpg",
    "default_title": "rakoon",
    "default_popup": "popup.html"
  },

  "content_scripts": [
    {
      "matches" : ["*://helplms.snu.ac.kr/learning/*"],
      "js" : ["jquery-3.1.1.min.js", "neogul.js"],
      "css" : ["neogul.css"]
    }
  ]
}
